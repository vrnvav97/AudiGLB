<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">
    <servlet>
        <servlet-name>History</servlet-name>
        <servlet-class>Servlets.History</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllowAudi</servlet-name>
        <servlet-class>Servlets.AllowAudi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelAudi</servlet-name>
        <servlet-class>Servlets.CancelAudi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelHistoryAdmin</servlet-name>
        <servlet-class>Servlets.CancelHistoryAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelHistoryUser</servlet-name>
        <servlet-class>Servlets.CancelHistoryUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AudiProcess</servlet-name>
        <url-pattern>/bookAudiProcess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>History</servlet-name>
        <url-pattern>/ViewHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllowAudi</servlet-name>
        <url-pattern>/AllowAudi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelAudi</servlet-name>
        <url-pattern>/CancelAudi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelHistoryAdmin</servlet-name>
        <url-pattern>/CancelHistoryAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelHistoryUser</servlet-name>
        <url-pattern>/CancelHistoryUser</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginCheck</servlet-name>
        <servlet-class>Servlets.LoginCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AudiProcess</servlet-name>
        <servlet-class>Servlets.AudiProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginCheck</servlet-name>
        <url-pattern>/LoginCheck</url-pattern>
    </servlet-mapping>
</web-app>
